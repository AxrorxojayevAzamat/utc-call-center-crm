<script>
    $(document).ready(() => {
        $(".icon-password-show").click(function () {
            $(this).toggleClass("mdi-eye-off");
            let type = $(this).hasClass("mdi-eye-off") ? "text" : "password";
            console.log($(this).prev())
            $(this).prev().attr("type", type);
        });
    })
</script>