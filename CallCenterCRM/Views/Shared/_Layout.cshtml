<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - CallCenterCRM</title>
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/css/select2.css" asp-append-version="true" />
	@*<link rel="stylesheet" href="~/css/select2-bootstrap.min.css" asp-append-version="true" />*@
	<link rel="stylesheet" href="~/css/icons/_icons.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/css/icons/_icons1.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/CallCenterCRM.styles.css" asp-append-version="true" />
</head>
<body>
	<partial name="Header" />
	<div class="container-fluid page-body-wrapper pt-0">
		<partial name="Sidebar" />
		<div class="main-panel">
			<div class="content-wrapper">
				<div class="row">
					<div class="card">
						<div class="card-body">
							
							@RenderBody()
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
